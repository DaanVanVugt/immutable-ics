{
  "name": "immutable-ics",
  "version": "0.3.0",
  "description": "Immutable iCalendar document creation",
  "main": "build/immutable-ics.js",
  "scripts": {
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write 'src/**/*.js'",
    "lint": "standard",
    "build": "webpack",
    "pretest": "npm run lint",
    "test": "mocha",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/angeloashmore/immutable-ics.git"
  },
  "keywords": [
    "ics",
    "calendar",
    "ical",
    "immutable"
  ],
  "author": "Angelo Ashmore",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/angeloashmore/immutable-ics/issues"
  },
  "homepage": "https://github.com/angeloashmore/immutable-ics#readme",
  "dependencies": {
    "lodash.isfunction": "^3.0.8",
    "lodash.isnull": "^3.0.0",
    "lodash.isundefined": "^3.0.1",
    "typed-immutable": "0.0.8"
  },
  "devDependencies": {
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "mocha": "^3.5.0",
    "prettier": "^1.5.3",
    "standard": "^7.1.2",
    "webpack": "^1.13.1"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ]
  },
  "standard": {
    "parser": "babel-eslint"
  }
}
