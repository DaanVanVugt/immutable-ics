{
  "name": "immutable-ics",
  "version": "0.4.0",
  "description": "Immutable iCalendar document creation",
  "main": "dist/immutable-ics.cjs.js",
  "module": "dist/immutable-ics.esm.js",
  "browser": "dist/immutable-ics.umd.js",
  "scripts": {
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write 'src/**/*.js' 'test/**/*.js' 'rollup.config.js'",
    "test": "NODE_ENV=test mocha",
    "build": "NODE_ENV=production rollup -c",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/angeloashmore/immutable-ics.git"
  },
  "keywords": [
    "ics",
    "calendar",
    "ical",
    "immutable"
  ],
  "author": "Angelo Ashmore",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/angeloashmore/immutable-ics/issues"
  },
  "homepage": "https://github.com/angeloashmore/immutable-ics#readme",
  "dependencies": {
    "lodash.isdate": "^4.0.1",
    "lodash.isfunction": "^3.0.8",
    "lodash.isnull": "^3.0.0",
    "lodash.isundefined": "^3.0.1",
    "lodash.padstart": "^4.6.1",
    "typed-immutable": "0.0.8"
  },
  "devDependencies": {
    "babel-core": "^6.10.4",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-preset-env": "^1.6.0",
    "mocha": "^3.5.0",
    "prettier": "^1.5.3",
    "rollup": "^0.48.2",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.2.0",
    "rollup-plugin-node-resolve": "^3.0.0"
  }
}
